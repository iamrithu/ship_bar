generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model shipBars {
  id          Int      @id @default(autoincrement())
  uuid        String   @unique @db.VarChar(255)
  name        String
  content     String
  createdAt   DateTime @default(now())
  shopDetails Shops    @relation(fields: [shop], references: [name])
  shop        String
}

model Shops {
  id      Int        @id @default(autoincrement())
  uuid    String     @unique @db.VarChar(255)
  shopId  String
  name    String     @unique
  product shipBars[]
}
